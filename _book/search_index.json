secheckout true #开启sparse checkout功能\necho \"子目录名称\" >> .git/info/sparse-checkout #添加需要拉取的目录名\ngit pull origin master #取远程master分支上的内容\n\nCopyright2019 all right reserved，powered by Gitbook2021.3.31\n2021.3.31\n"}}}